////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
MY SOLUTION
//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////


class Solution {
    public int missingNumber(int[] nums) {
        
        int n = nums.length;
        
        if(n==1){
            if(nums[0]==1){
                return 0;
            }
            return 1;
        }
        
        Set<Integer> set =  new HashSet<>();
        
        for(int i : nums){
            set.add(i);
        }
        
        for(int i=0;i<n;i++){
            if(!set.contains(i)){
                return i;
            }
        }
        return n;
    }
}
