class Solution {
    public void reverseString(char[] s) {
        
        int len = s.length;
        int start=0;
        int end=len-1;
        
        for(int i=0;i<s.length/2;i++){
            char temp = s[i];
            s[i]= s[end];
            s[end] =temp;
            
            end--;
        }
        //end at last index
        //start at 0th index
    }
}